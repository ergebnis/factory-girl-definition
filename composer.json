{
  "name": "localheinz/factory-girl-definition",
  "description": "Provides an interface for, and an easy way to find and register entity definitions for breerly/factory-girl-php.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "require": {
    "php": "^7.0",
    "localheinz/classy": "0.2.0"
  },
  "require-dev": {
    "breerly/factory-girl-php": "^1.0.0",
    "codeclimate/php-test-reporter": "0.4.4",
    "localheinz/php-cs-fixer-config": "~1.6.2",
    "localheinz/test-util": "0.2.1",
    "phpunit/phpunit": "^6.2.4"
  },
  "suggest": {
    "breerly/factory-girl-php": "For creating a fixture factory the definitions can be used with."
  },
  "autoload": {
    "psr-4": {
      "Localheinz\\FactoryGirl\\Definition\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Localheinz\\FactoryGirl\\Definition\\Test\\": "test"
    }
  }
}
